+++ b/plugins/dispatch-trace/main.c
@@ -121,7 +121,7 @@ dispatch_pcap_trace (vlib_main_t *vm, vlib_node_runtime_t *node,
 	  string_count++;
 
 	  /* Is this packet traced? */
